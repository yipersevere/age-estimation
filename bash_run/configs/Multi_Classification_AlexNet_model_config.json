{
  "data_loader": {
    "dataset": "CVPR_16_ChaLearn",
    "num_workers": 4,
    "loading_jobs": 4
  },
  "machine":{
    "working_machine": "Narvi"
  },
  "NN_hyperparameter":{
    "model": "MTL_AlexNet_model",
    "lr_schedule": 8,
    "batch_size": 8,
    "lr_rate": 0.001,
    "epoch": 40,
    "load_pretrained_model":"False",
    "weight_decay": 1e-6
  },
  "Multitask":{
    "multitask_training_type": "Train_Valid",
    "loss_weights": "[1,1,1,1]",
    "subtasks": ["gender", "smile", "emotion", "age"],
    "age_loss_type": "age_cls_loss"
  }
}
